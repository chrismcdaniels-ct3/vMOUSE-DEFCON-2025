# Amplify
NEXT_PUBLIC_AWS_REGION=us-east-1

# Unity Configuration
NEXT_PUBLIC_UNITY_LOADER_URL=/unity-builds/Build/UnityLoader.js

# S3 Configuration for Unity Files
NEXT_PUBLIC_UNITY_USE_S3=true
NEXT_PUBLIC_UNITY_S3_BUCKET=ct3-unity-webgl-assets
NEXT_PUBLIC_UNITY_S3_REGION=us-east-1
# CloudFront CDN URL (optional, for better performance)
# NEXT_PUBLIC_UNITY_CDN_URL=https://your-cloudfront-distribution.cloudfront.net
# Direct S3 URL
NEXT_PUBLIC_UNITY_BASE_URL=https://ct3-unity-webgl-assets.s3.amazonaws.com

# Analytics
NEXT_PUBLIC_ANALYTICS_ENABLED=true
